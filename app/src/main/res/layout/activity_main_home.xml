<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main_home"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.frank.smartparty.MainHome">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button26"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@android:color/holo_blue_dark"
        android:textSize="24sp"
        android:textColor="@android:color/background_dark"
        android:textStyle="normal|bold"
        android:text="聚会玩" />

    <Button
        android:text="我"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button27"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:background="@android:color/holo_blue_dark"
        android:textSize="24sp"
        android:layout_toEndOf="@+id/button26"
        android:textStyle="normal|bold" />

    <Button
        android:text="生活圈"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button28"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@android:color/holo_blue_dark"
        android:textSize="24sp"
        android:layout_toStartOf="@+id/button26"
        android:textStyle="normal|bold"
        android:textColor="@color/colorAccent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/textView14"
        android:text="阿隆索開發；；；；；；；；；；到底發生了對方律師是的理髮師理髮的隨大流風水獨領風騷的浪費啦士大夫士大夫是懶得發發牢騷地方塑料袋士大夫失戀分手了對方的老師傅收到士大夫\n\n\n\n这里就做成类似于QQ空间那种形式的，一个圈子里的人发表一些聚会的创意、景点信息等等之类的" />
</RelativeLayout>
